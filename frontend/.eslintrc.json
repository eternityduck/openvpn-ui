{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["eslint-plugin-prettier", "@typescript-eslint", "unused-imports"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ],
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all"
      }
    ],
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "react/jsx-key": [1, { "checkFragmentShorthand": true }]
  }
}